{{yield 'header'}}
{{#if model.certified}}
  <div class="{{model.certifiedClass}}">
    <span>{{model.certified}}</span>
  </div>
{{/if}}
{{#if showIcon}}
  <div class="catalog-icon">
    <img src="{{app.baseAssets}}assets/images/generic-catalog.svg" data-src={{model.links.icon}}/>
  </div>
{{/if}}
<div class="itemwrap mt-10">
  {{yield 'body'}}
</div>
<div class="footer">
  {{yield 'footer'}}
</div>
